{
  "name": "rnb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "flow": "./node_modules/.bin/flow check --show-all-errors",
    "flow-coverage":
      "./node_modules/.bin/flow-coverage-report -f ./node_modules/.bin/flow -i 'app/**/*.js'",
    "flow-typed-install": "./node_modules/.bin/flow-typed install",
    "eslint": "./node_modules/.bin/eslint ./app",
    "eslint:fix": "./node_modules/.bin/eslint --fix ./app",
    "eslint-check":
      "./node_modules/.bin/eslint --print-config .eslintrc.yaml | ./node_modules/.bin/eslint-config-prettier-check",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.52.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "22.0.6",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-react-native": "4.0.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.63.1",
    "flow-coverage-report": "^0.4.0",
    "flow-typed": "^2.2.3",
    "flowtype": "^2.0.0",
    "husky": "^0.14.3",
    "jest": "22.0.6",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
